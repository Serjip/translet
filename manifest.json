{
  "manifest_version": 2,

  "name": "Translet",
  "version": "1.0.1",

  "icons": {
    "16": "img/16x16.png",
    "32": "img/32x32.png",
    "49": "img/49x49.png",
    "128": "img/128x128.png",
    "256": "img/256x256.png",
    "512": "img/512x512.png"
  },
  "permissions": [
    "http://translate.yandex.net/*"
  ],

  "browser_action": {
    "default_title": "Translet",
    "default_icon": "img/49x49.png",
    "default_popup": "popup.html"
  }
}